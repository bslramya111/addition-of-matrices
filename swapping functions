#include <stdio.h>
void swap(int,int);
void swaping(int*,int*);
int main()
{
   int x,y;
   printf("enter x,y,value");
   scanf("%d %d",&x,&y);
   printf("before swapping\nx=%d,y=%d",x,y);
   swap(x,y);
   swaping(&x,&y);
   printf("after swapping\nx=%d,y=%d",x,y);
   return 0;
}
 void swap(int a,int b)
 {
     int temp;
     temp=b;
     b=a;
     a=temp;
     printf("values of a and b is:%d %d",a,b);
     
 }
 void swaping(int*p,int*q)
 {
     int temp;
     temp=*q;
     *q=*p;
     *p=temp;
     printf("values of p and q is:%d %d",p,q);
 }
